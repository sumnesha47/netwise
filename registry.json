{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "netwise",
  "homepage": "https://netwise.co.in",
  "items": [
    {
      "name": "password-input",
      "type": "registry:ui",
      "registryDependencies": ["input", "button"],
      "dependencies": ["react-icons"],
      "title": "Password Input",
      "description": "A password input component with visibility toggle.",
      "files": [
        {
          "path": "registry/ui/password-input.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "hello-world",
      "type": "registry:component",
      "title": "Hello World",
      "description": "A simple hello world component",
      "registryDependencies": ["button"],
      "files": [
        {
          "path": "registry/new-york/hello-world/hello-world.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "example-form",
      "type": "registry:component",
      "title": "Example Form",
      "description": "A contact form with Zod validation.",
      "dependencies": ["zod"],
      "registryDependencies": ["button", "input", "label", "textarea", "card"],
      "files": [
        {
          "path": "registry/new-york/example-form/example-form.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "complex-component",
      "type": "registry:component",
      "title": "Complex Component",
      "description": "A complex component showing hooks, libs and components.",
      "registryDependencies": ["card"],
      "files": [
        {
          "path": "registry/new-york/complex-component/page.tsx",
          "type": "registry:page",
          "target": "app/pokemon/page.tsx"
        },
        {
          "path": "registry/new-york/complex-component/components/pokemon-card.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/new-york/complex-component/components/pokemon-image.tsx",
          "type": "registry:component"
        },
        {
          "path": "registry/new-york/complex-component/lib/pokemon.ts",
          "type": "registry:lib"
        },
        {
          "path": "registry/new-york/complex-component/hooks/use-pokemon.ts",
          "type": "registry:hook"
        }
      ]
    },
    {
      "name": "hono-setup",
      "type": "registry:block",
      "title": "Hono Setup",
      "description": "setting up Hono with a simple hello world route.",
      "dependencies": ["hono"],
      "files": [
        {
          "path": "registry/new-york/hono-setup/api/[[...route]]/route.ts",
          "type": "registry:file",
          "target": "src/app/api/[[...route]]/route.ts"
        }
      ]
    },
    {
      "name": "tanstack-query-setup",
      "type": "registry:block",
      "title": "Tanstack Query Setup",
      "description": "",
      "dependencies": ["@tanstack/react-query"],
      "files": [
        {
          "path": "registry/new-york/tanstack-query-setup/query-provider.tsx",
          "type": "registry:component"
        }
      ]
    }
  ]
}
